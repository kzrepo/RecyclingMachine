@model IEnumerable<Machine.Database.Data.AdminSite.Voucher>

@{
    ViewData["Title"] = "Vouchers";
}

<h1>@ViewData["Title"]</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdVoucher)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Session)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Start)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.End)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdVoucher)
            </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Session)
                </td>
            <td>
                @Html.DisplayFor(modelItem => item.Start)
            </td>
                <td>
                    @Html.DisplayFor(modelItem => item.End)
                </td>
        </tr>
}
    </tbody>
</table>
